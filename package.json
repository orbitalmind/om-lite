{
  "name": "om-lite",
  "version": "0.1.0",
  "description": "Structured, decay-aware external memory for OpenClaw agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    }
  },
  "bin": {
    "om-lite": "./dist/cli/index.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepack": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "docs": "typedoc --out docs/api src/index.ts"
  },
  "keywords": [
    "orbitalmind",
    "openclaw",
    "ai-agent",
    "memory",
    "llm",
    "knowledge-graph",
    "personal-assistant",
    "decay-aware",
    "structured-memory"
  ],
  "author": "Orbital Mind",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/orbitalmind/om-lite.git"
  },
  "bugs": {
    "url": "https://github.com/orbitalmind/om-lite/issues"
  },
  "homepage": "https://github.com/orbitalmind/om-lite#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist",
    "packs",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "date-fns": "^3.3.0",
    "glob": "^10.3.0",
    "ora": "^8.0.0",
    "sqlite-vec": "^0.1.7-alpha.2",
    "uuid": "^11.1.0",
    "yaml": "^2.4.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.3.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.4.0",
    "vitest": "^1.3.0"
  },
  "peerDependencies": {
    "openai": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    }
  }
}
